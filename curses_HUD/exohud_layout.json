{
    "settings": {"debugmode": true},

    "stylesheet": {
        "colors": {
            "black": {"rgb": [0, 0, 0]},
            "white": {"rgb": [1000, 1000, 1000]},
            "red": {"rgb": [1000, 0, 0]},
            "green": {"rgb": [0, 1000, 0]}
    },
        "colorpairs": {
            "black_white": {"colorpair": ["black", "white"]},
            "mycolor0": {"colorpair": [20, 100]},
            "tealtext": {"colorpair": [50, 0]},
            "redbg": {"colorpair": ["white", "red"]},
            "greenbg": {"colorpair": ["white", "green"]}
        }
    },

    "layout": {
        "HUD": {
            "widgettype": "HUD",
            "nlines": 30,
            "ncols": 62,
            "l": 0,
            "c": 0,
            "signature": "h",
            "colorpair": "tealtext",
            "kwargs": {"bgchar": " ", "bordertype": "box"},
            "parent": ""
        },

        "widgets": {
            "hudtitle": {
                "widgettype": "TextBox",
                "nlines": 1,
                "ncols": 15,
                "l": 1,
                "c": 3,
                "kwargs": {"bgchar": " ", "textlines": ["Exoboot HUD"]},
                "signature": "ht",
                "parent": "h"
            },

            "subjinfo": {
                "widgettype": "TextBox",
                "nlines": 1,
                "ncols": 40,
                "l": 3,
                "c": 4,
                "signature": "si",
                "kwargs": {"useprefix": true, "lineprefix": ["Info: "]},
                "parent": "h"
            },

            "ipinfo": {
                "widgettype": "TextBox",
                "nlines": 1,
                "ncols": 20,
                "l": 4,
                "c": 4,
                "signature": "ii",
                "kwargs": {"useprefix": true, "lineprefix": ["IP: "]},
                "parent": "h"
            },

            "pausebtn": {
                "widgettype": "ToggleButton",
                "nlines": 2,
                "ncols": 8,
                "l": 5,
                "c": 40,
                "signature": "pb",
                "colorpair": "greenbg",
                "kwargs": {"bgchar": "P", "textlines": ["pausebtn", "pausebtn"], "onpressfunc": "setpauseevent", "requires_top": false, "togglestate": false},
                "parent": "h"
            },

            "quitbtn": {
                "widgettype": "ToggleButton",
                "nlines": 2,
                "ncols": 7,
                "l": 5,
                "c": 50,
                "signature": "qb",
                "colorpair": "greenbg",
                "kwargs": {"bgchar": "Q", "textlines": ["quitbtn", "quitbtn"], "onpressfunc": "setquitevent", "requires_top": false, "togglestate": false},
                "parent": "h"
            },

            "exol_status_window": {
                "widgettype": "Window",
                "nlines": 13,
                "ncols": 20,
                "l": 16,
                "c": 1,
                "signature": "elsw",
                "kwargs": {"bgchar": " ", "bordertype": "box", "isdragable": true, "isresizeable": true, "keepbottomborder": true},
                "parent": "h"
            },

            "exol_title": {
                "widgettype": "TextBox",
                "nlines": 1,
                "ncols": 18,
                "l": 1,
                "c": 1,
